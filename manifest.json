{
  "name": "豆瓣小组自动审批",
  "description": "根据小组暗号自动审批入组",
  "manifest_version": 3,
  "version": "1.0.0",
  "icons": {
    "128": "/images/logo.png"
  },
  "permissions": ["storage", "activeTab", "declarativeContent", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "/images/logo.png"
    }
  }
}
